#include "stdafx.h"
#include <iostream>
#include "stdafx.h"

using namespace std;

void cargar(int vec[],int n);
void mostrar(int vec[],int n);

void main()
{int vec[20],n;
cout<<"Ingrese el tamaÃ±o del vector: ";
cin>>n;
cargar(vec,n);
mostrar(vec,n);
}

void cargar(int vec[],int n)
{ int i;
for(i=0;i<n;i++)
{
cout<<"Ingrese un valor";
cin>>vec[i];
}
}
void mostrar(int vec[],int n)
{
	int i;
	for (i=0;i<n;i++)
	{cout<<vec[i];
	cout<<endl;
	}
}
